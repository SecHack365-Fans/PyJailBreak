{
  "name": "webconsole",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "export": "next export",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "lint": "next lint",
    "prepare": "cd .. && husky install webconsole/.husky",
    "start": "next start"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn run lint"
    ],
    "*.{js,jsx,ts,tsx,md,json}": [
      "yarn run format"
    ]
  },
  "dependencies": {
    "next": "12.2.3",
    "next-swagger-doc": "^0.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swagger-ui-react": "^4.13.0"
  },
  "devDependencies": {
    "@types/node": "18.0.6",
    "@types/react": "18.0.15",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "4.7.4"
  }
}
